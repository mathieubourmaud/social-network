{
    "extends": ["airbnb"],
    "plugins": ["jest"],
    "env": {
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "rules": {
      "no-underscore-dangle": [ "error", {"allow": ["_id"]} ],
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/label-has-for": 0,
      "jsx-a11y/label-has-associated-control": 0,
      "react/forbid-prop-types": 0
    },
    "globals": {
      "document": true,
      "window": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": ["src"]
        }
      }
    }
  }
  